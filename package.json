{
  "name": "html-table",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "cssnano": "^4.1.7",
    "inline-assets": "^1.4.1",
    "lodash": "^4.17.11",
    "nodemon": "^1.18.7",
    "parcel": "^1.10.3",
    "parcel-bundler": "^1.10.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-jsx-parser": "^1.13.0",
    "react-table": "^6.8.6"
  },
  "scripts": {
    "build-app": "rm -rf app-dist && parcel build --no-source-maps app-template/index.template.html --public-url '.' --out-dir app-dist && inline-assets app-dist/index.template.html > app-dist/index.html && rm app-dist/*.{js,css,template.html}",
    "watch-app": "nodemon --watch app-template --ext js,html,css --exec yarn build-app"
  },
  "bin": {
    "html-table": "./run"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "cheerio": "^1.0.0-rc.2",
    "minimist": "^1.2.0",
    "opn": "^5.4.0",
    "tempy": "^0.2.1"
  }
}
